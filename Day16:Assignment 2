#include <stdio.h>

int main() {
    int n, k, i, evenSum=0 , oddSum=0 ;
    
    printf("Enter the value of n=");
    scanf("%d",&n);
    printf("Enter the value of k=");
    scanf("%d", &k);
    printf("%d %d\n", n, k);
    
    for (i = 1; i <= n; i++)
	 {
        if (i % k == 0) 
		{
            printf("%d\n", i);
            
            if (i % 2 == 0)
			        {
                evenSum += i;
               } 
               else
			        {
                oddSum += i;
                }
       }
    }
    
    printf("Even sum: %d\n", evenSum);
    printf("Odd sum: %d\n", oddSum);
    
    return 0;
}
