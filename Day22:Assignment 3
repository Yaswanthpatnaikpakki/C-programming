#include <stdio.h>

int main()
 {
  int n, i;

  printf("Enter a number: ");
  scanf("%d", &n);
   printf("Numbers from 1 to %d: ", n);
  for (i = 1; i <= n; i++)
   {
    printf("%d ", i);
  }
  
printf("\n");
   printf("Numbers from %d to 1: ", n);
  for (i = n; i >= 1; i--)
   {
    printf("%d ", i);
  }
   return 0;
}
