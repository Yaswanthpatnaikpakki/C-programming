
#include <stdio.h>

int main() {
    int n, max1, max2, num;
    printf("Enter the value of n: ");
    scanf("%d", &n);
    max1 = max2 = -1;
    printf("Enter %d numbers:\n", n);
    for(int i = 0; i < n; i++) {
        scanf("%d", &num);
        if (num > max1) {
            max2 = max1;
            max1 = num;
        } else if (num > max2 && num < max1) {
            max2 = num;
        }
    }
    printf("The second highest number is: %d\n", max2);
    return 0;
}
